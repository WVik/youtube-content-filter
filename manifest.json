{
  "manifest_version": 2,

  "name": "YouTube Content Filter",
  "description": "This extension shows a Google Image search result for the current page",
  "version": "1.0",
  "icons":{"128":"main.png"},

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "https://ajax.googleapis.com/"
  ],
  "content_scripts": [{
        "matches": ["<all_urls>"],
        "all_frames": true,
        "js":      ["jq.js", "./content.js"]
    }]
  }
